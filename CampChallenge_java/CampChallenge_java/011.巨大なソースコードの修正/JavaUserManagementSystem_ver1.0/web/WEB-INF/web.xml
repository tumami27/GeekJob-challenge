<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>insert</servlet-name>
        <servlet-class>jums.Insert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertconfirm</servlet-name>
        <servlet-class>jums.InsertConfirm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertresult</servlet-name>
        <servlet-class>jums.InsertResult</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JavaBeans</servlet-name>
        <servlet-class>JavaBeans</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserrDateBeans</servlet-name>
        <servlet-class>jums.UserrDateBeans</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insert</servlet-name>
        <url-pattern>/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertconfirm</servlet-name>
        <url-pattern>/insertconfirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertresult</servlet-name>
        <url-pattern>/insertresult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JavaBeans</servlet-name>
        <url-pattern>/JavaBeans</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserrDateBeans</servlet-name>
        <url-pattern>/UserrDateBeans</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
